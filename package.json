{
    "name": "werfen-touch-client-prod",
    "description": "WERFEN TOUCH with Electron + Create React App + Electron Builder",
    "version": "10.7.93-beta",
    "author": "Werfen",
    "private": true,
    "scripts": {
        "start": "craco start",
        "build": "craco build",
        "test": "mocha",
        "electron-dev": "concurrently \"cross-env BROWSER=none yarn start\" \"wait-on http://localhost:3000 && electron .\"",
        "postinstall": "electron-builder install-app-deps",
        "preelectron-pack-w": "copy .env .\\public\\.env&&set PUBLIC_URL=./&&yarn build",
        "electron-pack-w": "electron-builder -w",
        "test:one": "mocha --watch -u bdd \\\"build/test/**/test-*.js\\\" --grep \\\"test-navigation\\\""
    },
    "never": {
        "eject": "react-scripts eject"
    },
    "build": {
        "target": "nsis",
        "publish": [
            {
                "provider": "github",
                "owner": "naoufalWerfen",
                "repo": "werfen-touch-client",
                "token": "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"
            }
        ],
        "appId": "com.werfen.touch",
        "productName": "WerfenTouch 3.0 (PROD)",
        "copyright": "Copyright Â© 2022 ${author}",
        "nsis": {
            "artifactName": "WerfenTouch 3.0 Setup build (${version}).${ext}",
            "runAfterFinish": true
        },
        "win": {
            "target": [
                {
                    "target": "nsis",
                    "arch": [
                        "x64"
                    ]
                }
            ],
            "icon": "./assets/icons/icon.ico",
            "certificateSubjectName": "WERFENLIFE SA",
            "signingHashAlgorithms": [
                "sha256"
            ],
            "publisherName": "WERFENLIFE SA",
            "signAndEditExecutable": true,
            "timeStampServer": "https://timestamp.globalsign.com/tsa/r6advanced1"
        },
        "files": [
            "build/**/*",
            "node_modules/**/*"
        ],
        "directories": {
            "buildResources": "assets"
        },
        "generateUpdatesFilesForAllChannels": false,
        "releaseInfo": {
            "releaseNotes": "Standard release, updates2"
        }
    },
    "eslintConfig": {
        "extends": "react-app"
    },
    "browserslist": {
        "production": [
            ">0.2%",
            "not dead",
            "not op_mini all"
        ],
        "development": [
            "last 1 chrome version",
            "last 1 firefox version",
            "last 1 safari version"
        ]
    },
    "externals": {
        "sqlite3": "commonjs2 sqlite3"
    },
    "main": "public/electron.js",
    "dependencies": {
        "@sentry/electron": "^4.2.0",
        "@sentry/react": "^7.36.0",
        "@sentry/tracing": "^7.36.0",
        "@testing-library/jest-dom": "^4.2.4",
        "@testing-library/react": "^9.5.0",
        "@testing-library/user-event": "^7.2.1",
        "atob": "^2.1.2",
        "axios": "^0.21.1",
        "better-sqlite3": "7.4.0",
        "bootstrap": "^4.5.0",
        "chmodr": "^1.2.0",
        "craco": "^0.0.3",
        "decompress-zip": "^0.3.3",
        "electron-dl": "^3.0.0",
        "electron-download-manager": "ssh://git@github.com/ofurmanowska/electron-download-manager-remake",
        "electron-is-dev": "^1.2.0",
        "electron-log": "^4.3.4",
        "electron-push-receiver": "^2.1.3",
        "electron-updater": "4.3.9",
        "file-saver": "^2.0.5",
        "form-data": "^4.0.0",
        "formik": "^2.2.6",
        "fs": "0.0.1-security",
        "fs-extra": "^9.0.1",
        "history": "^4.10.1",
        "i18next": "^20.2.2",
        "is-online": "^9.0.0",
        "knex": "^2.3.0",
        "mobx": "^6.7.0",
        "mobx-react-lite": "^3.4.0",
        "moment": "^2.29.1",
        "node-blob": "^0.0.2",
        "node-gyp": "^8.4.1",
        "normalize-path": "^3.0.0",
        "os-locale": "^5.0.0",
        "react": "^16.13.1",
        "react-bootstrap": "^1.0.1",
        "react-bootstrap-table-next": "^4.0.3",
        "react-bootstrap-table2-filter": "^1.3.3",
        "react-custom-alert": "^0.1.3",
        "react-datepicker": "^4.2.1",
        "react-dom": "^16.13.1",
        "react-i18next": "^11.8.15",
        "react-markdown": "^8.0.2",
        "react-piwik": "^1.8.0",
        "react-redux": "^7.2.0",
        "react-router-dom": "^5.2.0",
        "react-router-hash-link": "^1.2.2",
        "react-router-redux": "^4.0.8",
        "react-scripts": "3.4.1",
        "react-update-url-on-scroll": "^3.5.0",
        "redux": "^4.0.5",
        "redux-devtools-dock-monitor": "^1.1.3",
        "redux-devtools-log-monitor": "^1.4.0",
        "redux-logger": "^3.0.6",
        "redux-thunk": "^2.3.0",
        "rimraf": "^3.0.2",
        "sass": "^1.49.8",
        "semver": "^7.3.5",
        "uuid": "^8.3.2",
        "yup": "^0.32.8"
    },
    "devDependencies": {
        "@types/mocha": "^8.2.0",
        "chai": "^4.2.0",
        "chai-as-promised": "^7.1.1",
        "concurrently": "^5.2.0",
        "cross-env": "^7.0.2",
        "electron": "^9.4.4",
        "electron-builder": "^22.11.3",
        "electron-chromedriver": "^11.0.0",
        "electron-devtools-installer": "^3.2.0",
        "is-electron": "^2.2.0",
        "mocha": "^8.2.1",
        "node-static": "^0.7.11",
        "redux-devtools": "^3.5.0",
        "spectron": "^13.0.0",
        "typescript": "^3.9.5",
        "wait-on": "^5.2.1",
        "yarn": "^1.22.17"
    },
    "babel": {
        "presets": [
            "react-app"
        ]
    }
}
